<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.User" table="user">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property name="sid" type="java.lang.Integer" update="true" insert="true" column="sid" />
		<property name="facebook_user_id" type="java.lang.Long" update="true" insert="true" column="facebook_user_id"/>
	</class>
</hibernate-mapping>