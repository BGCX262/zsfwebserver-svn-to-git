<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.Enemy" table="enemy">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property name="masterID" type="java.lang.Long" column="masterID"/>
		<property name="enemyID" type="java.lang.Long" column="enemyID"/>
		<property name="time" type="java.lang.Long" column="time"/>
	</class>
</hibernate-mapping>