<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.UserProp" table="userprop">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property name="masterID" type="java.lang.Long" column="masterID"/>
		<property name="propID" type="java.lang.Integer" column="propID"/>
		<property name="useTime" type="java.lang.Long" column="useTime"/>
		<property name="expiration" type="java.lang.Long" column="expiration"/>
	</class>
</hibernate-mapping>