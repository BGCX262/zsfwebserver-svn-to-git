<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.Race" table="race">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="assigned"/>
		</id>
		<property name="workPerMin" type="java.lang.Integer" column="workPerMin"/>
		<property name="repairNeedCoin" type="java.lang.Integer" column="repairNeedCoin"/>
		<property name="repairNeedRock" type="java.lang.Integer" column="repairNeedRock"/>
		<property name="repairNeedMetal" type="java.lang.Integer" column="repairNeedMetal"/>
		<property name="repairNeedCrystal" type="java.lang.Integer" column="repairNeedCrystal"/>
	</class>
</hibernate-mapping>