<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.PickedFriends" table="pickedfriends">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="assigned"/>
		</id>
		<property name="time" type="java.lang.Long" column="time"/>
		<property name="info" type="java.lang.String" column="info" length="1024"/>
	</class>
</hibernate-mapping>