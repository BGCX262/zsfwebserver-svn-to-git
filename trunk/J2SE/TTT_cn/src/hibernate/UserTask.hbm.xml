<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.UserTask" table="usertask">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property name="masterID" type="java.lang.Long" column="masterID"/>
		<property name="taskID" type="java.lang.Integer" column="taskID"/>
		<property name="state" type="java.lang.Integer" column="state"/>
		<property name="taskType" type="java.lang.Integer" column="taskType"/>
		<property name="finishRate" type="java.lang.Double" column="finishRate"/>
		<property name="acceptTime" type="java.lang.Long" column="acceptTime"/>
		<property name="finishTime" type="java.lang.Long" column="finishTime"/>
		<property name="lucky" type="java.lang.Integer" column="lucky"/>
	</class>
</hibernate-mapping>