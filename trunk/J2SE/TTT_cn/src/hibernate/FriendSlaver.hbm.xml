<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.FriendSlaver" table="friendslaver">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="identity"/>
		</id>
		<property name="masterID" column="masterID" type="java.lang.Long" />
		<property name="friendID" column="friendID" type="java.lang.Long" />
		<property name="escapeTime" type="java.lang.Long" column="escapeTime"/>
		<property name="speedUp" type="java.lang.Integer" column="speedUp"/>
	</class>
</hibernate-mapping>