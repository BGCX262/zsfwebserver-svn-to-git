<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.UserSoulTower" table="usersoultower">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="assigned"/>
		</id>
		<property name="state" type="java.lang.Integer" column="state"/>
		<property name="soulTowerLevel" type="java.lang.Integer" column="soulTowerLevel"/>
		<property name="endTime" type="java.lang.Long" column="endTime"/>
	</class>
</hibernate-mapping>