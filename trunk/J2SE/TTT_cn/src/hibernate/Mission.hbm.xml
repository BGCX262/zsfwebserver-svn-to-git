<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.Mission" table="mission">
		<composite-id>
			<key-property name="id" type="java.lang.Integer" column="id"></key-property>
			<key-property name="masterID" type="java.lang.Long" column="masterID"></key-property>
		</composite-id>
		<property name="product" type="java.lang.Integer" column="product"/>
		<property name="endTime" type="java.lang.Long" column="endTime"/>
		<property name="num" type="java.lang.Integer" column="num"/>
	</class>
</hibernate-mapping>