<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.UserPickfriend" table="userpickfriend">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="identity"/>
		</id>
		<property name="masterID" type="java.lang.Long" column="masterID"/>
		<property name="friendID" type="java.lang.Long" column="friendID"/>
		<property name="pickedNum" type="java.lang.Integer" column="pickedNum"/>
		<property name="lastPickedTime" type="java.lang.Long" column="lastPickedTime"/>
	</class>
</hibernate-mapping>