<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.TitleBean" table="titlebean">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="identity"/>
		</id>
		<property name="goodID" type="java.lang.Integer" column="goodID"/>
		<property name="cate" type="java.lang.Integer" column="cate"/>
		<property name="glory" type="java.lang.Integer" column="glory"/>
		<property name="title" type="java.lang.String" column="title"/>
		<property name="resourcename" type="java.lang.String" column="resourcename"  length="50"/>
		<property name="preTitle" type="java.lang.Integer" column="preTitle"/>
		<property name="slaverID" type="java.lang.Integer" column="slaverID"/>
		<property name="coinCost" type="java.lang.Integer" column="coinCost"/>
		<property name="comment" type="java.lang.String" column="comment" length="100"/>
	</class>
</hibernate-mapping>