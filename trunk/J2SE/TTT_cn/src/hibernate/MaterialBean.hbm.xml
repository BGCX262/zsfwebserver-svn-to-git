<?xml version="1.0"  encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.database.model.bean.MaterialBean" table="materialbean">
		<id name="goodID" column="goodID" type="java.lang.Integer">
			<generator class="assigned"/>
		</id>
		<property name="cate" type="java.lang.Integer" column="cate"/>
		<property name="name" type="java.lang.String" column="name" length="50"/>
		<property name="resourceName" type="java.lang.String" column="resourceName" length="50"/>
		<property name="comment" type="java.lang.String" column="comment" length="100"/>
	</class>
</hibernate-mapping>